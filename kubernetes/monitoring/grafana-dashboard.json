{
  "dashboard": {
    "id": null,
    "title": "Katya Mesh Network Monitoring",
    "tags": ["katya", "mesh", "networking"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Active Peers",
        "type": "stat",
        "targets": [
          {
            "expr": "mesh_peers_active",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 80
                }
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Messages Per Second",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(mesh_messages_total[5m])",
            "refId": "A"
          }
        ]
      },
      {
        "id": 3,
        "title": "Network Latency",
        "type": "graph",
        "targets": [
          {
            "expr": "mesh_network_latency_seconds",
            "refId": "A"
          }
        ]
      },
      {
        "id": 4,
        "title": "CPU Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(process_cpu_user_seconds_total[5m])",
            "refId": "A"
          }
        ]
      },
      {
        "id": 5,
        "title": "Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "process_resident_memory_bytes",
            "refId": "A"
          }
        ]
      },
      {
        "id": 6,
        "title": "Protocol Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "mesh_protocol_messages_total",
            "refId": "A"
          }
        ]
      },
      {
        "id": 7,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(mesh_errors_total[5m])",
            "refId": "A"
          }
        ]
      },
      {
        "id": 8,
        "title": "Queue Size",
        "type": "graph",
        "targets": [
          {
            "expr": "mesh_queue_size",
            "refId": "A"
          }
        ]
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    },
    "refresh": "5s",
    "schemaVersion": 27,
    "version": 0,
    "links": []
  }
}

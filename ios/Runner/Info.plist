	<string>Main</string>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>UISupportedInterfaceOrientations~ipad</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationPortraitUpsideDown</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>

	<!-- iOS Platform Specific Features -->
	<key>UIStatusBarHidden</key>
	<false/>
	<key>UIViewControllerBasedStatusBarAppearance</key>
	<false/>

	<!-- Background Modes -->
	<key>UIBackgroundModes</key>
	<array>
		<string>fetch</string>
		<string>processing</string>
		<string>remote-notification</string>
	</array>

	<!-- Background Tasks -->
	<key>BGTaskSchedulerPermittedIdentifiers</key>
	<array>
		<string>com.katyaairechainmesh.refresh</string>
		<string>com.katyaairechainmesh.sync</string>
	</array>

	<!-- Push Notifications -->
	<key>aps-environment</key>
	<string>development</string>

	<!-- Local Notifications -->
	<key>UNUserNotificationCenterDelegate</key>
	<string>$(PRODUCT_MODULE_NAME).AppDelegate</string>

	<!-- Security and Privacy -->
	<key>NSFaceIDUsageDescription</key>
	<string>This app uses Face ID for secure authentication</string>
	<key>NSCameraUsageDescription</key>
	<string>This app needs camera access to capture photos and videos</string>
	<key>NSMicrophoneUsageDescription</key>
	<string>This app needs microphone access for voice features</string>
	<key>NSPhotoLibraryUsageDescription</key>
	<string>This app needs photo library access to save and load images</string>
	<key>NSLocationWhenInUseUsageDescription</key>
	<string>This app needs location access to provide location-based features</string>
	<key>NSLocationAlwaysAndWhenInUseUsageDescription</key>
	<string>This app needs location access to provide location-based features</string>
	<key>NSContactsUsageDescription</key>
	<string>This app needs contacts access to enhance social features</string>
	<key>NSRemindersUsageDescription</key>
	<string>This app needs reminders access for scheduling notifications</string>
	<key>NSCalendarsUsageDescription</key>
	<string>This app needs calendar access for event scheduling</string>

	<!-- Biometric Authentication -->
	<key>NSFaceIDUsageDescription</key>
	<string>Enable Face ID for secure authentication</string>

	<!-- Keychain Access -->
	<key>keychain-access-groups</key>
	<array>
		<string>$(AppIdentifierPrefix)com.katyaairechainmesh</string>
	</array>

	<!-- App Transport Security -->
	<key>NSAppTransportSecurity</key>
	<dict>
		<key>NSAllowsArbitraryLoads</key>
		<false/>
		<key>NSAllowsArbitraryLoadsInWebContent</key>
		<false/>
		<key>NSAllowsLocalNetworking</key>
		<true/>
	</dict>

	<!-- WebKit Features -->
	<key>WKAppBoundDomains</key>
	<array>
		<string>katyaairechainmesh.com</string>
	</array>

	<!-- URL Schemes -->
	<key>CFBundleURLTypes</key>
	<array>
		<dict>
			<key>CFBundleTypeRole</key>
			<string>Editor</string>
			<key>CFBundleURLName</key>
			<string>com.katyaairechainmesh</string>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>katya-rechain-mesh</string>
				<string>katyaairechainmesh</string>
			</array>
		</dict>
	</array>

	<!-- File Sharing -->
	<key>LSSupportsOpeningDocumentsInPlace</key>
	<false/>
	<key>UIFileSharingEnabled</key>
	<false/>

	<!-- iCloud -->
	<key>NSUbiquitousContainers</key>
	<dict>
		<key>iCloud.com.katyaairechainmesh</key>
		<dict>
			<key>NSUbiquitousContainerIsDocumentScopePublic</key>
			<true/>
			<key>NSUbiquitousContainerName</key>
			<string>Katya AI REChain Mesh</string>
			<key>NSUbiquitousContainerSupportedFolderLevels</key>
			<string>Any</string>
		</dict>
	</dict>

	<!-- Core Spotlight -->
	<key>CoreSpotlightContinuation</key>
	<true/>

	<!-- Siri Integration -->
	<key>NSUserActivityTypes</key>
	<array>
		<string>INSendMessageIntent</string>
		<string>INSearchForMessagesIntent</string>
	</array>

	<!-- Widget Support -->
	<key>NSExtension</key>
	<dict>
		<key>NSExtensionPointIdentifier</key>
		<string>com.apple.widgetkit-extension</string>
		<key>NSExtensionPrincipalClass</key>
		<string>$(PRODUCT_MODULE_NAME).WidgetExtension</string>
	</dict>

	<!-- Associated Domains -->
	<key>com.apple.developer.associated-domains</key>
	<array>
		<string>webcredentials:katyaairechainmesh.com</string>
		<string>applinks:katyaairechainmesh.com</string>
	</dict>


	<key>com.apple.developer.networking.networkextension</key>
	<array>
		<string>content-filter-provider-systemextension</string>
		<string>dns-proxy-systemextension</string>
	</array>

	<!-- DriverKit -->
	<key>com.apple.developer.driverkit</key>
	<true/>
	<key>com.apple.developer.driverkit.transport.usb</key>
	<true/>

	<!-- HomeKit -->
	<key>NSHomeKitUsageDescription</key>
	<string>This app needs HomeKit access for smart home integration</string>

	<!-- ClassKit -->
	<key>com.apple.developer.ClassKit-environment</key>
	<string>development</string>

	<!-- Game Center -->
	<key>com.apple.developer.game-center</key>
	<true/>

	<!-- WeatherKit -->
	<key>com.apple.developer.weatherkit</key>
	<true/>

	<!-- Maps -->
	<key>com.apple.developer.maps</key>
	<true/>

	<!-- Sign In with Apple -->
	<key>com.apple.developer.signin.apple</key>
	<true/>

	<!-- Developer Mode -->
	<key>com.apple.developer.developer-mode</key>
	<true/>

	<!-- Required Device Capabilities -->
	<key>UIRequiredDeviceCapabilities</key>
	<array>
		<string>arm64</string>
		<string>metal</string>
	</array>

